// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./user.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id String @id @default(cuid())
  fullName String
  email String 
  password String
}
